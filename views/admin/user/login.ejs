<div class="container">
  <div class="row">
    <div class="col-4"></div>
    <div class="col-4 login" id="">
      <h3>
        <p class="text-black-50 display-6">
          <strong>Adm - Login</strong>
        </p>
      </h3>
      <form id="form-login">
        <div class="col mb-3 form-group">
          <label for="login-email" class="form-label">Email</label>
          <input
            type="email"
            class="form-control"
            name="login-email"
            id="login-email"
            aria-describedby="emailHelp"
            placeholder="exemplo@exemplo.com"
          />
          <div id="emailHelp" class="form-text"></div>
        </div>
        <div class="col mb-3 form-group">
          <label for="login-password" class="form-label">Senha</label>
          <input
            type="password"
            class="form-control"
            name="login-password"
            id="login-password"
            placeholder="********"
          />
          <div id="passwordlHelp" class="form-text">
            Sua senha deve conter no mÃ­nimo 8 caracteres
          </div>
        </div>
        <div class="form-group">
          <button type="button" class="btn btn-dark bg-dark" id="bt-login">
            Entrar
          </button>
        </div>
      </form>
    </div>
  </div>
</div>

<script src="/js/admin/login.shineyou.js"></script>
